VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "Sheet2"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Private Sub CheckBox1_Click()

End Sub

Private Sub CommandButton1_Click()

Dim A As String
Dim B As Long
Dim c As Long
Dim D As Long
Dim E As Long
Dim AR As Range
Dim CH As Long
A = InputBox("パレット数を記入してください")
c = 0
D = 600
E = 1

Sheets("詳細").Range("A66:A74").Copy

If A = "" Then
MsgBox ("パレット数が入力されていません")
Exit Sub
Else
If CheckBox1.Value = True Then CH = 2
Set AR = Sheets("DATA").Cells(Rows.Count, 3).End(xlUp).Offset(1, 5 - CH)

For B = 1 To 18



Select Case B


Case 2, 3, 5, 6, 7, 8

AR.Offset(B - 1, 0).Value = 300 * A

Case 1, 9

AR.Offset(B - 1, 0).Value = D * A

Case 4

AR.Offset(B - 1, 0).Value = 900 * A



Case 10

AR.Offset(B - 10, 1).Value = A

Case 11 To 18

AR.Offset(B - 10, 1).Value = 0

End Select


Next


Cells(Rows.Count, 3).End(xlUp).Offset(1, 0).PasteSpecial

End If
End Sub

